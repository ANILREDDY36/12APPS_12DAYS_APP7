<div id="category">
  <div class="category">
    <h2>
      <%= @category.name %>
    </h2>
  </div>
</div>
<div id="links">
  <%= link_to "Back", root_path, class: "btn btn-sm btn-default" %>
  <%= link_to "Edit", edit_category_path(@category), class: "btn btn-sm btn-default" %>
  <%= link_to "Delete", category_path(@category), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-sm btn-default" %>
</div>
